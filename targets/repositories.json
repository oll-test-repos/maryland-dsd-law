{
    "scopes": ["us/md/exec"],
    "repositories": {
        "maryland-dsd/law-docs": {
            "custom": {
                "type":"docs",
                "allow-unauthenticated-commits":true,
                "serve":"latest",
                "location_regex":"~/.*\\.pdf$",
                "routes": [".*\\.pdf"]
            }
        },
        "maryland-dsd/law-xml": {
            "custom": {
                "type":"xml",
                "allow-unauthenticated-commits":true,
                "serve":"latest",
                "serve-prefix":"_uncodified_xml"
            }
        },
        "maryland-dsd/law-rdf": {
            "custom": {
                "type":"rdf",
                "serve":"historical",
                "serve-prefix":"_rdf"
            }
        },
        "maryland-dsd/law-html": {
            "custom": {
                "preview":"git@github.com:maryland-dsd/law-html-preview.git",
                "type":"html",
                "serve":"historical",
                "location_regex":"/",
                "routes": [".*"]
            }
        },
        "maryland-dsd/law-xml-codified": {
            "custom": {
                "preview":"git@github.com:maryland-dsd/law-xml-codified-preview.git",
                "type":"xml-codified",
                "serve":"historical",
                "serve-prefix":"_xml"
            }
        },
        "maryland-dsd/law-static-assets": {
            "custom": {
                "type":"static-assets",
                "allow-unauthenticated-commits":true,
                "serve":"latest",
                "location_regex":"~.+(_document|_reader).+$",
                "is_fallback": true,
                "routes": [".*/_document/.*", "_reader/.*"]
            }
        }
    }
}